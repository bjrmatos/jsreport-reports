{
  "name": "jsreport-reports",
  "version": "1.0.2",
  "description": "jsreport extension adding support for storing rendering outputs for later use",
  "scripts": {
    "test": "mocha test --timeout=10000 && standard",
    "build": "npm run prepublish",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-reports",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-reports.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.4.7",
    "in-publish": "2.0.0",
    "node.extend": "1.1.5",
    "underscore": "1.8.3",
    "url": "0.11.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "6.0.5",
    "eslint": "2.13.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "jsreport-core": "1.0.4",
    "jsreport-express": "1.0.2",
    "jsreport-studio-dev": "1.0.0",
    "mocha": "2.5.3",
    "should": "9.0.2",
    "standard": "7.1.2",
    "supertest": "1.2.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
